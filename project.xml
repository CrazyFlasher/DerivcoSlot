<?xml version="1.0" encoding="utf-8"?>
<project>
	
	<meta title="DerivcoSlot" package="com.sample.derivcoslot" version="1.0.0" company="Company Name" />
	<app main="Main" path="Export" file="DerivcoSlot" />

	<window hardware="true" allow-shaders="true" require-shaders="true" depth-buffer="true" stencil-buffer="true" fps="60" width="1280"
			height="720" background="#FFFFFF"/>

	<window if="mobile" orientation="landscape" fullscreen="true" />
	<window if="html5" resizable="false" />

	<source path="Source" />

	<haxeflag name="-dce full" />

	<haxelib name="openfl" />
	<haxelib name="actuate" />
	<haxelib name="haxeui-core" />
	<haxelib name="haxeui-openfl" />
	<haxelib name="as3hx" />

	<assets path="Assets" rename="assets" exclude="*.swf"/>

	<library path="assets/ui.swf" id="ui" generate="true" preload="true"/>
</project>